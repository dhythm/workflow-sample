{
  "name": "workflow-sample",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:create": "docker-compose run -p 5432:5432 -d postgres",
    "db:schema": "prisma generate",
    "db:seed": "ts-node-esm --project tsconfig.node.json prisma/seed.ts",
    "db:init": "rm -fr prisma/migrations && prisma migrate dev --name init && yarn run db:seed",
    "db:migrate": "prisma migrate deploy --preview-feature"
  },
  "dependencies": {
    "@prisma/client": "4.11.0",
    "@types/node": "18.14.6",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "cors": "^2.8.5",
    "eslint": "8.35.0",
    "eslint-config-next": "13.2.3",
    "next": "13.2.3",
    "react": "18.2.0",
    "react-async": "^10.0.1",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.5",
    "react-query": "^3.39.3",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "eslint-config-prettier": "^8.6.0",
    "prettier": "2.8.4",
    "prisma": "^4.11.0",
    "ts-node": "^10.9.1"
  }
}
